@font-face {
    font-family: 'open_sansregular';
    src: url('<%= asset_path("OpenSans-Regular-webfont.eot") %>');
    src: url('<%= asset_path("OpenSans-Regular-webfont.eot?#iefix") %>') format('embedded-opentype'),
         url('<%= asset_path("OpenSans-Regular-webfont.svg#open_sansregular") %>') format('svg'),
         url('<%= asset_path("OpenSans-Regular-webfont.woff") %>') format('woff'),
         url('<%= asset_path("OpenSans-Regular-webfont.ttf") %>') format('truetype');
    font-weight: normal;
    font-style: normal;
}


